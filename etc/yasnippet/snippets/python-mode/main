import argparse


def main(args):
    pass


if __name__ == '__main__':
    parser = argparse.ArgumentParser()
    parser.add_argument('path')
    parser.add_argument('--version', '-v',
                        action='store_true', default=None,
                        help='Print version information')
    args = parser.parse_args()
    main(args)
